{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "esnext",
    "module": "Node16",
    "lib": ["es6"],
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "jsx": "react-native",
    "types": ["jest"]
  },
  "include": ["__test__", "src", "jest.config.ts"]
}
